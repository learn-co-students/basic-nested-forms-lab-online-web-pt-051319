<%= form_for @recipe do |f| %>
    Title: <%= f.text_field :title %>
    <br>
    Ingredients: <br>
    <%= f.fields_for :ingredients do |i| %>
        <%= i.label :quantity %>
        <%= i.text_field :quantity %>
        <%= i.label :name %>
        <%= i.text_field :name %>
        <% end %>
        <%= f.submit %>
        <% end %>